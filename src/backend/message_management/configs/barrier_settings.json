{
  "RTU5025": {
    "start": {
      "name": "Начало работы",
      "description": "Запускает GSM модуль, включает в работу.",
      "template": "{pwd}TEL0007{local_phone}",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "example": "1234",
          "description": "Пароль от устройства - 4 цифры."
        },
        {
          "name": "Номер установленной в устройстве SIM-карты",
          "key": "local_phone",
          "example": "9854500040",
          "description": "Стандартный российский номер телефона, без пробелов, без +7, начинается например с 999"
        }
      ],
      "example": "1234TEL00079854500040"
    },
    "password_change": {
      "name": "Смена пароля",
      "description": "",
      "template": "{pwd}P{newpwd}",
      "params": [
        {
          "name": "Текущий пароль",
          "key": "pwd",
          "example": "1234",
          "description": ""
        },
        {
          "name": "Новый пароль",
          "key": "newpwd",
          "example": "1234",
          "description": ""
        }
      ],
      "example": "1234P5678"
    },
    "get_phone": {
      "name": "Запрос номера в списке",
      "description": "",
      "template": "{pwd}A{index}#",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "description": "",
          "example": "1234"
        },
        {
          "name": "Серийный номер — положение номера в списке (от 001 до 999).",
          "key": "index",
          "description": "",
          "example": "003"
        }
      ],
      "example": "1234A003#"
    },
    "get_phones": {
      "name": "Запрос нескольких номеров в списке",
      "template": "{pwd}AL{start_index}#{end_index}#",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "description": "",
          "example": "1234"
        },
        {
          "name": "Начальный серийный номер ",
          "key": "start_index",
          "description": "",
          "example": "003"
        },
        {
          "name": "Конечный серийный номер",
          "key": "end_index",
          "description": "",
          "example": "050"
        }
      ],
      "example": "1234AL003#050#"
    },
    "all_access": {
      "name": "Управление по звонку с любого номера",
      "description": "",
      "template": "{pwd}ALL#",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "description": "",
          "example": "1234"
        }
      ],
      "example": "1234ALL#"
    },
    "restrict_access": {
      "name": "Управление по звонку только с разрешенных номеров",
      "description": "",
      "template": "{pwd}AUT#",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "description": "",
          "example": "1234"
        }
      ],
      "example": "1234AUT#"
    },
    "close_time": {
      "name": "Сколько времени держать закрытым после звонка от 000 до 999 в секундах",
      "description": "",
      "template": "{pwd}GOT{time}#",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "description": "",
          "example": "1234"
        },
        {
          "name": "Время до открытия в секундах",
          "key": "time",
          "description": "",
          "example": "030"
        }
      ],
      "example": "1234GOT030#"
    },
    "turn_on_messages": {
      "name": "Включить сообщения об изменении состояния",
      "description": "",
      "template": "{pwd}G{condition}{who}{msg}#",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "description": "",
          "example": "1234"
        },
        {
          "name": "На какое состояние нужны сообщения? ON - на закрытое, OFF на открытое",
          "key": "condition",
          "description": "",
          "example": "ON"
        },
        {
          "name": "Кому отправлять сообщения? Две цифры, первая говорит отправлять ли первому номеру в таблице, вторая говорит отправлять ли тому кто позвонил. 0 это не отправлять, 1 это отправлять",
          "key": "who",
          "description": "",
          "example": "11"
        },
        {
          "name": "Какое сообщение отправлять",
          "key": "msg",
          "description": "",
          "example": "Door Open"
        }
      ],
      "example": "1234GON11#Door Open##"
    },
    "turn_off_messages": {
      "name": "Отключить сообщения об изменении состояния",
      "description": "",
      "template": "{pwd}G{condition}##",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "description": "",
          "example": "1234"
        },
        {
          "name": "На какое состояние нужны сообщения? ON - на закрытое, OFF на открытое",
          "key": "condition",
          "description": "",
          "example": "ON"
        }
      ],
      "example": "1234GON##"
    },
    "open_barrier": {
      "name": "Открыть шлагбаум",
      "description": "",
      "template": "{pwd}DD",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "description": "",
          "example": "1234"
        }
      ],
      "example": "1234DD"
    },
    "close_barrier": {
      "name": "Закрыть шлагбаум",
      "description": "",
      "template": "{pwd}CC",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "description": "",
          "example": "1234"
        }
      ],
      "example": "1234CC"
    }
  },
  "Telemetrica": {
    "start": {
      "name": "Начало работы",
      "template": "#0#",
      "params": [],
      "example": "#0#"
    },
    "get_phones": {
      "name": "Запрос списка номеров телефона",
      "template": "#06#",
      "params": [],
      "example": "#06#"
    },
    "all_access": {
      "name": "Управление по звонку с любого номера",
      "template": "#31#1#",
      "params": [],
      "example": "#31#1#"
    },
    "restrict_access": {
      "name": "Управление по звонку только с разрешенных номеров",
      "template": "#31#0#",
      "params": [],
      "example": "#31#0#"
    },
    "close_time": {
      "name": "Сколько времени держать закрытым после звонка от 000 до 999 в секундах",
      "template": "{pwd}GOT{time}#",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd",
          "validation": "^\\d{4}$",
          "example": "1234"
        },
        {
          "name": "Время до открытия в секундах",
          "key": "time",
          "validation": "^\\d{3}$",
          "example": "030"
        }
      ],
      "example": "1234GOT030#"
    }
  }
}
