{
  "RTU5025": {
    "add_phone": {
      "name": "Добавить номер телефона",
      "template": "{pwd}A{index}#0007{phone}#",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd"
        },
        {
          "name": "Серийный номер (от 001 до 999)",
          "key": "index"
        },
        {
          "name": "Номер телефона",
          "key": "phone"
        }
      ],
      "example": "1234A003#00079854500040#"
    },
    "delete_phone": {
      "name": "Удалить номер телефона",
      "template": "{pwd}A{index}##",
      "params": [
        {
          "name": "Пароль",
          "key": "pwd"
        },
        {
          "name": "Серийный номер",
          "key": "index"
        }
      ],
      "example": "1234A003##"
    }
  },
  "Telemetrica": {
    "add_phone": {
      "name": "Добавить номер телефона",
      "template": "#60#+7{phone}#",
      "params": [
        {
          "name": "Номер телефона",
          "key": "phone"
        }
      ],
      "example": "#60#+79854500040#"
    },
    "delete_phone": {
      "name": "Удалить номер телефона",
      "template": "#15#+7{phone}#",
      "params": [
        {
          "name": "Номер телефона",
          "key": "phone"
        }
      ],
      "example": "#15#+79854500040#"
    }
  }
}
